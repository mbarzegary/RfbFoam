FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      invPermeability;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 0 0 0 0];

internalField   uniform 0; 
//internalField   uniform 0.00; 

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;

    }
    currColl
    {
        type            zeroGradient;
    }
    mem
    {
        type            zeroGradient;
    }  
 
}



