FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "1";
    object      CR;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 250;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 250;
    }
    outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
    currColl
    {
        type            zeroGradient;
    }
    mem
    {
        type            zeroGradient;
    }
}


